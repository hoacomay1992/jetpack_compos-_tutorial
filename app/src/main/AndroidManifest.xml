            android:name=".ui.ButtonComposeActivity"
            android:exported="true"
            android:label="@string/title_activity_button_compose"
            android:theme="@style/Theme.JpackComponentComposeTutorial" />
        <activity
            android:name=".ui.ImageActivity"
            android:exported="true"
            android:label="@string/title_activity_image"
            android:theme="@style/Theme.JpackComponentComposeTutorial" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.JpackComponentComposeTutorial" />
    </application>

</manifest>