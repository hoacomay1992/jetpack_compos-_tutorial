        <activity
            android:name=".ui.DialogComposeActivity"
            android:exported="true"
            android:label="@string/title_activity_dialog_compose"
            android:theme="@style/Theme.JpackComponentComposeTutorial" />
        <activity
            android:name=".ui.ChipComposeActivity"
            android:exported="true"
            android:label="@string/title_activity_chip_compose"
            android:theme="@style/Theme.JpackComponentComposeTutorial" />
        <activity
            android:name=".ui.CardComposeActivity"
